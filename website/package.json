{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sync:release-assets": "bash -lc 'set -euo pipefail; mkdir -p public; VERSION=$(sed -n \"s/^version = \\\"\\(.*\\)\\\"/\\1/p\" ../Cargo.toml | head -n1); test -n \"$VERSION\"; printf \"%s\\n\" \"${MATO_VERSION:-$VERSION}\" > public/version.txt; cp ../install.sh public/install.sh'",
    "build": "npm run sync:release-assets && next build",
    "dev": "npm run sync:release-assets && next dev",
    "start": "npm run sync:release-assets && next start",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "fumadocs-core": "16.6.4",
    "fumadocs-mdx": "14.2.7",
    "fumadocs-ui": "16.6.4",
    "lucide-react": "^0.570.0",
    "next": "16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
